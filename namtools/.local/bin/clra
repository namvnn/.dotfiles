#!/usr/bin/env bash

# Author: Nam Nguyen <vietnam.n@proton.me>
# Description: Print 256 terminal colors
# License: MIT License
# Date Created: October 21, 2024
# Date Modified: January 26, 2026

output=''

for i in {0..255}; do
    output="${output}\033[38;5;${i}m${i}\n"
done

printf "${output}" | column
