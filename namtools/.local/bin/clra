#!/usr/bin/env bash

# Author: Nam Nguyen <nguyenvietnam2401@gmail.com>
# Description: Print 256 terminal colors
# License: MIT License
# Date created: October 21, 2024
# Date modified: January 12, 2026

output=''

for i in {0..255}; do
    output="${output}\033[38;5;${i}m${i}\n"
done

printf "${output}" | column
