#!/usr/bin/env bash

# Author: Nam Nguyen <nam@namn.me>
# Description: Print 256 terminal colors
# License: MIT License
# Date Created: October 21, 2024
# Date Modified: February 13, 2026

output=''

for i in {0..255}; do
    output="${output}\033[38;5;${i}m${i}\n"
done

printf "${output}" | column
